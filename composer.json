{
    "name": "daljo25/whatsapp-rich-editor",
    "description": "This is my package whatsapp-rich-editor",
    "keywords": [
        "daljo25",
        "laravel",
        "whatsapp-rich-editor"
    ],
    "homepage": "https://github.com/daljo25/whatsapp-rich-editor",
    "support": {
        "issues": "https://github.com/daljo25/whatsapp-rich-editor/issues",
        "source": "https://github.com/daljo25/whatsapp-rich-editor"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "daljo25",
            "email": "camalion007@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.1",
        "filament/forms": "^3.0",
        "spatie/laravel-package-tools": "^1.15.0"
    },
    "require-dev": {
        "nunomaduro/collision": "^7.9",
        "orchestra/testbench": "^8.0",
        "pestphp/pest": "^2.1",
        "pestphp/pest-plugin-arch": "^2.0",
        "pestphp/pest-plugin-laravel": "^2.0"
    },
    "autoload": {
        "psr-4": {
            "Daljo25\\WhatsappRichEditor\\": "src/",
            "Daljo25\\WhatsappRichEditor\\Database\\Factories\\": "database/factories/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Daljo25\\WhatsappRichEditor\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": "@php ./vendor/bin/testbench package:discover --ansi",
        "test": "vendor/bin/pest",
        "test-coverage": "vendor/bin/pest --coverage"
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "phpstan/extension-installer": true
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Daljo25\\WhatsappRichEditor\\WhatsappRichEditorServiceProvider"
            ],
            "aliases": {
                "WhatsappRichEditor": "Daljo25\\WhatsappRichEditor\\Facades\\WhatsappRichEditor"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}